{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/web-desktop/index.html"
    }
  ],
  "headers": [
    {
      "source": "/web-desktop/(.*.wasm)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ]
}